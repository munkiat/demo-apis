  "oas": {
    "components": {},
    "info": {
      "title": "products",
      "version": "1.0.0"
    },
    "openapi": "3.0.3",
    "paths": {
      "/": {
        "get": {
          "operationId": "get",
          "responses": {
            "200": {
              "description": ""
            }
          }
        }
      }
    },
    "servers": [
      {
        "url": "http://localhost:8080/checkProduct/"
      }
    ],
    "x-tyk-api-gateway": {
      "info": {
        "dbId": "668f82f1019f4500018d93dd",
        "id": "7d0a59c60b094d936b0a2a23daf0eebe",
        "name": "checkProducts",
        "orgId": "668f8266019f4500018d93d9",
        "state": {
          "active": true
        }
      },
      "middleware": {
        "global": {
          "contextVariables": {
            "enabled": true
          }
        },
        "operations": {
          "get": {
            "urlRewrite": {
              "enabled": true,
              "pattern": "/checkProduct/(.*)",
              "rewriteTo": "https://httpbin.org/get",
              "triggers": [
                {
                  "condition": "all",
                  "rewriteTo": "tyk://toys/images/jpeg",
                  "rules": [
                    {
                      "in": "requestBody",
                      "pattern": "\"productType\": \"Toys\""
                    }
                  ]
                },
                {
                  "condition": "all",
                  "rewriteTo": "tyk://electronics/images/png",
                  "rules": [
                    {
                      "in": "requestBody",
                      "pattern": "\"productType\": \"Electronics\""
                    }
                  ]
                }
              ]
            }
          }
        }
      },
      "server": {
        "detailedActivityLogs": {
          "enabled": true
        },
        "gatewayTags": {
          "enabled": true,
          "tags": [
            "gateway1"
          ]
        },
        "listenPath": {
          "strip": true,
          "value": "/checkProduct/"
        }
      },
      "upstream": {
        "url": "https://httpbin.org/"
      }
    }
  },
  "hook_references": [],
  "is_site": false,
  "sort_by": 0,
  "user_group_owners": [],
